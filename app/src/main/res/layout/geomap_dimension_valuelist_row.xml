<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/linearLayout2"    android:layout_width="fill_parent"    android:layout_height="wrap_content"    android:background="@color/black" >    <TextView        android:id="@+id/dimensionValueListItem"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:textColor="@color/white"        android:textSize="12sp" />    <CheckBox        android:id="@+id/dimensionValueCheckbox"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentRight="true"        android:layout_alignParentEnd="true"        android:button="@drawable/geo_checkbox"        android:layout_centerVertical="true"        /></RelativeLayout>